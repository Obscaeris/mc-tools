{
  "name": "obscaeris.com",
  "homepage": "https://obscaeris.github.io/mc-tools",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --cors --port 60000",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "tsc -b && vite build --outDir build",
    "lint": "npx eslint --report-unused-disable-directives src -f gha",
    "lint-fix": "npx eslint --report-unused-disable-directives --fix src -f gha",
    "tailwind": "npx tailwindcss -i ./public/styles/main.css -o ./public/styles/tailwind.css --watch",
    "preview": "vite preview"
  },
  "dependencies": {
    "@stylistic/eslint-plugin-js": "^2.6.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.24.1",
    "typescript-eslint": "^8.2.0"
  },
  "devDependencies": {
    "@cascadia-code/scroll-to-hash-element": "^2.0.2",
    "@eslint/js": "^9.9.1",
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "@prisma/client": "^5.9.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-formatter-gha": "^1.5.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "gh-pages": "^6.1.1",
    "prisma": "^5.9.1",
    "tailwindcss": "^3.4.5",
    "tw-colors": "^3.3.1",
    "typescript": "^5.2.2",
    "vite": "^5.3.1"
  }
}
